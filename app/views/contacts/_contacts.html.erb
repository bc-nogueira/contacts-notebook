<h2>Contatos</h2>

<table class="table">
  <thead>
  <tr>
    <th>#</th>
    <th>Nome</th>
    <th>E-mail</th>
    <th>Data de Nascimento</th>
  </tr>
  </thead>
  <tbody>
  <% contacts.each do |contact| %>
      <tr>
        <th scope="row"><%= contact.id %></th>
        <td><%= contact.name %></td>
        <td><%= contact.email %></td>
        <td><%= contact.birthdate %></td>
      </tr>
  <% end %>
  </tbody>
</table>